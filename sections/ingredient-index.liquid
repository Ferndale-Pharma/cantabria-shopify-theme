{% paginate shop.metaobjects.ingredients.values by 25 %}
  
    {% for metaobject in shop.metaobjects.chef.values %}
      <li>
        {{ metaobject.name | link_to: metaobject.system.url }}
      </li>
    {% endfor %}
  
{{ paginate | default_pagination }}

{% endpaginate %}
{% schema %}
    {
      "name": "Ingredient Index",
      "settings": [
        {
          "type": "text",
          "id": "title",
          "label": "Title",
          "default": "Ingredients"
        }
      ],
      "presets": [
        {
          "name": "Default",
          "category": "Custom"
        }
      ]
    }
    {% endschema %}